{
    "version": 2,
    "env": {
        "MONGO_URI": "@shop_api_mongo_uri"
    },
    "functions": {
        "/(.*)": {
          "memory": 1024,
          "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}
  